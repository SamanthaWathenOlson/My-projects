package entities;

public class Producer {
    int myInt;
    String myString;
    boolean myBoolean;

public class Producer {
    private String fullName;

    public Entities(String fullName)  {
        setFullName(fullName);

    public void println() {
        System.out.println("Sarah O'Connor");
    }
    
    
    
public class Customer {
    int myInt;
    String myString;
    boolean myBoolean;


public class Customer {
    private String fullName;

    public Entities(String fullName)  {
        setFullName(fullName);

    public void println() {
        System.out.println("John O'Connor");
    }

    

